{
    "name": "pterodactyl-addons/ollama-ai",
    "description": "Comprehensive AI integration addon for Pterodactyl Panel using local Ollama models",
    "type": "library",
    "license": "MIT",
    "keywords": ["pterodactyl", "addon", "ai", "ollama", "artificial-intelligence", "local-ai"],
    "authors": [
        {
            "name": "Pterodactyl Addons",
            "email": "addons@pterodactyl.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "PterodactylAddons\\OllamaAi\\": "src/"
        }
    },
    "extra": {
        "pterodactyl": {
            "minimum_version": "1.11.0",
            "maximum_version": "1.99.99"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}